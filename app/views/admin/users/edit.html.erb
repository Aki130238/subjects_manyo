<div class="container">
  <h1>user編集画面</h1>
  <%= form_with(model: [ :admin, @user ], local: true) do |f| %>  
    <%= f.label :name %>
    <%= f.text_field :name, id: :user_name %>
  
    <%= f.label :email %>
    <%= f.email_field :email, id: :user_email %>
  
    <%= f.submit "Create my account" %>
  <% end %>
</div>
